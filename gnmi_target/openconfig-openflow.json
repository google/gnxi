{
  "openconfig-interfaces:interfaces": {
    "interface": [
      {
        "name": "admin",
        "config": {
          "name": "admin"
        }
      }
    ]
  },
  "openconfig-system:system": {
    "aaa": {
      "authentication": {
        "admin-user": {
          "config": {
            "admin-password": "password"
          }
        },
        "config": {
          "authentication-method": [
            "openconfig-aaa-types:LOCAL"
          ]
        }
      }
    },
    "clock": {
      "config": {
        "timezone-name": "Europe/Stockholm"
      }
    },
    "config": {
      "hostname": "zz-tri-dev01",
      "domain-name": "foo.bar.com",
      "login-banner": "This device is for authorized use only",
      "motd-banner": "Welcome to Open vSwitch"
    },
    "openconfig-openflow:openflow": {
      "agent": {
        "config": {
          "backoff-interval": 5,
          "datapath-id": "00:16:3e:00:00:00:00:00",
          "failure-mode": "SECURE",
          "inactivity-probe": 10,
          "max-backoff": 10
        }
      },
      "controllers": {
        "controller": [
          {
            "config": {
              "name": "main"
            },
            "connections": {
              "connection": [
                {
                  "aux-id": 0,
                  "config": {
                    "address": "192.0.2.10",
                    "aux-id": 0,
                    "port": 6633,
                    "priority": 1,
                    "source-interface": "admin",
                    "transport": "TLS"
                  },
                  "state": {
                    "address": "192.0.2.10",
                    "aux-id": 0,
                    "port": 6633,
                    "priority": 1,
                    "source-interface": "admin",
                    "transport": "TLS"
                  }
                }
              ]
            },
            "name": "main"
          }
        ]
      }
    }
  }
}
